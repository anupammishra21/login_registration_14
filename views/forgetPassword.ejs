<%- include('../layouts/header') %>

<div class="registration-form">




    <a style="text-align: center;" href="/dashboard" class="btn btn-success">Back-To-Dashboard</a>



    <form method="post" action="/PasswordUpdate">
        <div class="form-icon">
            <span><i class="icon icon-user"></i></span>
            <!-- <div class="heading">Registration</div> -->
        </div>

        <div class="form-group">
            <input type="email" class="form-control item" id="email" name="email"
                placeholder="Enter your email">
        </div>



        <div class="form-group">
            <button type="submit" class="btn btn-block create-account">Save</button>
        </div>

    </form>


    <%- include('../layouts/footer') %>